.PHONY: main trace_line transit_surface

all: main trace_line transit_surface
binary: main trace_line transit_surface
opt: main trace_line transit_surface
compd: main trace_line transit_surface
match: main trace_line transit_surface

main:
	@echo
	${MAKE} -C main $(MAKECMDGOALS)
trace_line:
	@echo
	${MAKE} -C trace_line $(MAKECMDGOALS)
transit_surface:
	@echo
	${MAKE} -C transit_surface $(MAKECMDGOALS)
