.PHONY: advance energy offset_momentum main

all: advance energy offset_momentum main
binary: advance energy offset_momentum main
opt: advance energy offset_momentum main
compd: advance energy offset_momentum main
match: advance energy offset_momentum main

advance:
	@echo
	${MAKE} -C advance $(MAKECMDGOALS)
energy:
	@echo
	${MAKE} -C energy $(MAKECMDGOALS)
offset_momentum:
	@echo
	${MAKE} -C offset_momentum $(MAKECMDGOALS)
main:
	@echo
	${MAKE} -C main $(MAKECMDGOALS)
