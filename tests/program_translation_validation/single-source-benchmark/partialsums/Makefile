.PHONY: make_vec sum_vec main

all: make_vec sum_vec main
binary: make_vec sum_vec main
opt: make_vec sum_vec main
compd: make_vec sum_vec main
match: make_vec sum_vec main

make_vec:
	@echo
	${MAKE} -C make_vec $(MAKECMDGOALS)
sum_vec:
	@echo
	${MAKE} -C sum_vec $(MAKECMDGOALS)
main:
	@echo
	${MAKE} -C main $(MAKECMDGOALS)
