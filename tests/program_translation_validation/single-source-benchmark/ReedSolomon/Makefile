.PHONY: rsdec_204 generate_gf gen_poly decode_rs rsenc_204 encode_rs main

all: rsdec_204 generate_gf gen_poly decode_rs rsenc_204 encode_rs main
binary: rsdec_204 generate_gf gen_poly decode_rs rsenc_204 encode_rs main
opt: rsdec_204 generate_gf gen_poly decode_rs rsenc_204 encode_rs main
compd: rsdec_204 generate_gf gen_poly decode_rs rsenc_204 encode_rs main
match: rsdec_204 generate_gf gen_poly decode_rs rsenc_204 encode_rs main

rsdec_204:
	@echo
	${MAKE} -C rsdec_204 $(MAKECMDGOALS)
generate_gf:
	@echo
	${MAKE} -C generate_gf $(MAKECMDGOALS)
gen_poly:
	@echo
	${MAKE} -C gen_poly $(MAKECMDGOALS)
decode_rs:
	@echo
	${MAKE} -C decode_rs $(MAKECMDGOALS)
rsenc_204:
	@echo
	${MAKE} -C rsenc_204 $(MAKECMDGOALS)
encode_rs:
	@echo
	${MAKE} -C encode_rs $(MAKECMDGOALS)
main:
	@echo
	${MAKE} -C main $(MAKECMDGOALS)
