.PHONY: Initrand Rand rInitmatrix rInnerproduct Mm main

all: Initrand Rand rInitmatrix rInnerproduct Mm main
binary: Initrand Rand rInitmatrix rInnerproduct Mm main
opt: Initrand Rand rInitmatrix rInnerproduct Mm main
compd: Initrand Rand rInitmatrix rInnerproduct Mm main
match: Initrand Rand rInitmatrix rInnerproduct Mm main

Initrand:
	@echo
	${MAKE} -C Initrand $(MAKECMDGOALS)
Rand:
	@echo
	${MAKE} -C Rand $(MAKECMDGOALS)
rInitmatrix:
	@echo
	${MAKE} -C rInitmatrix $(MAKECMDGOALS)
rInnerproduct:
	@echo
	${MAKE} -C rInnerproduct $(MAKECMDGOALS)
Mm:
	@echo
	${MAKE} -C Mm $(MAKECMDGOALS)
main:
	@echo
	${MAKE} -C main $(MAKECMDGOALS)
