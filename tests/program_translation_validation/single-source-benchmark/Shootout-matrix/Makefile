.PHONY: mkmatrix zeromatrix freematrix mmult main

all: mkmatrix zeromatrix freematrix mmult main
binary: mkmatrix zeromatrix freematrix mmult main
opt: mkmatrix zeromatrix freematrix mmult main
compd: mkmatrix zeromatrix freematrix mmult main
match: mkmatrix zeromatrix freematrix mmult main

mkmatrix:
	@echo
	${MAKE} -C mkmatrix $(MAKECMDGOALS)
zeromatrix:
	@echo
	${MAKE} -C zeromatrix $(MAKECMDGOALS)
freematrix:
	@echo
	${MAKE} -C freematrix $(MAKECMDGOALS)
mmult:
	@echo
	${MAKE} -C mmult $(MAKECMDGOALS)
main:
	@echo
	${MAKE} -C main $(MAKECMDGOALS)
