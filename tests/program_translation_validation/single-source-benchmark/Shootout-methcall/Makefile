.PHONY: toggle_value toggle_activate init_Toggle new_Toggle nth_toggle_activate init_NthToggle new_NthToggle main

all: toggle_value toggle_activate init_Toggle new_Toggle nth_toggle_activate init_NthToggle new_NthToggle main
binary: toggle_value toggle_activate init_Toggle new_Toggle nth_toggle_activate init_NthToggle new_NthToggle main
opt: toggle_value toggle_activate init_Toggle new_Toggle nth_toggle_activate init_NthToggle new_NthToggle main
compd: toggle_value toggle_activate init_Toggle new_Toggle nth_toggle_activate init_NthToggle new_NthToggle main
match: toggle_value toggle_activate init_Toggle new_Toggle nth_toggle_activate init_NthToggle new_NthToggle main

toggle_value:
	@echo
	${MAKE} -C toggle_value $(MAKECMDGOALS)
toggle_activate:
	@echo
	${MAKE} -C toggle_activate $(MAKECMDGOALS)
init_Toggle:
	@echo
	${MAKE} -C init_Toggle $(MAKECMDGOALS)
new_Toggle:
	@echo
	${MAKE} -C new_Toggle $(MAKECMDGOALS)
nth_toggle_activate:
	@echo
	${MAKE} -C nth_toggle_activate $(MAKECMDGOALS)
init_NthToggle:
	@echo
	${MAKE} -C init_NthToggle $(MAKECMDGOALS)
new_NthToggle:
	@echo
	${MAKE} -C new_NthToggle $(MAKECMDGOALS)
main:
	@echo
	${MAKE} -C main $(MAKECMDGOALS)
