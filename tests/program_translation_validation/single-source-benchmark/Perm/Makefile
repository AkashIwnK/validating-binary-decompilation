.PHONY: Initrand Rand Swap Initialize Permute Perm main

all: Initrand Rand Swap Initialize Permute Perm main
binary: Initrand Rand Swap Initialize Permute Perm main
opt: Initrand Rand Swap Initialize Permute Perm main
compd: Initrand Rand Swap Initialize Permute Perm main
match: Initrand Rand Swap Initialize Permute Perm main

Initrand:
	@echo
	${MAKE} -C Initrand $(MAKECMDGOALS)
Rand:
	@echo
	${MAKE} -C Rand $(MAKECMDGOALS)
Swap:
	@echo
	${MAKE} -C Swap $(MAKECMDGOALS)
Initialize:
	@echo
	${MAKE} -C Initialize $(MAKECMDGOALS)
Permute:
	@echo
	${MAKE} -C Permute $(MAKECMDGOALS)
Perm:
	@echo
	${MAKE} -C Perm $(MAKECMDGOALS)
main:
	@echo
	${MAKE} -C main $(MAKECMDGOALS)
