.PHONY: main init noise fade grad lerp

all: main init noise fade grad lerp
binary: main init noise fade grad lerp
opt: main init noise fade grad lerp
compd: main init noise fade grad lerp
match: main init noise fade grad lerp

main:
	@echo
	${MAKE} -C main $(MAKECMDGOALS)
init:
	@echo
	${MAKE} -C init $(MAKECMDGOALS)
noise:
	@echo
	${MAKE} -C noise $(MAKECMDGOALS)
fade:
	@echo
	${MAKE} -C fade $(MAKECMDGOALS)
grad:
	@echo
	${MAKE} -C grad $(MAKECMDGOALS)
lerp:
	@echo
	${MAKE} -C lerp $(MAKECMDGOALS)
