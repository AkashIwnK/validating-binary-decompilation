.PHONY: ReverseBits32 ReverseBits64 main

all: ReverseBits32 ReverseBits64 main
binary: ReverseBits32 ReverseBits64 main
opt: ReverseBits32 ReverseBits64 main
compd: ReverseBits32 ReverseBits64 main
match: ReverseBits32 ReverseBits64 main

ReverseBits32:
	@echo
	${MAKE} -C ReverseBits32 $(MAKECMDGOALS)
ReverseBits64:
	@echo
	${MAKE} -C ReverseBits64 $(MAKECMDGOALS)
main:
	@echo
	${MAKE} -C main $(MAKECMDGOALS)
